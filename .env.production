# Configuración de Producción - TallerPro Premium
# Reemplaza estos valores con tu configuración real

# URL de tu dominio personalizado
VITE_APP_URL=https://tudominio.com

# Configuración Supabase (Sistema de Pagos)
VITE_SUPABASE_URL=https://tu-proyecto.supabase.co
VITE_SUPABASE_ANON_KEY=tu_clave_publica_supabase

# Configuración Firebase (Opcional - para usuarios avanzados)
VITE_FIREBASE_API_KEY=tu_api_key_firebase
VITE_FIREBASE_AUTH_DOMAIN=tu-proyecto.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=tu-proyecto-firebase
VITE_FIREBASE_STORAGE_BUCKET=tu-proyecto.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123:web:abc123

# Modo de producción
NODE_ENV=production
VITE_APP_ENV=production

# Configuración de pagos
